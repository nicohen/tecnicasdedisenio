<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 

    <description>
      Trabajo Practico de Tecnicas de Diseño Grupo 5: La Rematada
    </description>
    <display-name>LaRematada</display-name>


    <servlet>
        <servlet-name>BidControllerServlet</servlet-name>
        <servlet-class>test.web.bid.BidControllerServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>api.web.login.Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>BidControllerServlet</servlet-name>
        <url-pattern>/BidControllerServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
</web-app>